{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-delete-var":"error",
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "no-console": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "quotes": [
            "error",            
            "double"
        ],
        "indent": ["error", "tab"],
        "semi":"warn",
        "no-mixed-spaces-and-tabs":"warn"
    },
    "extends": [
        "eslint:recommended"
      ]
}